/* MY Custom Styles */
/*overide bootstrap*/


table {
  margin: 0;
}
ul {
  margin: 0;
}
.form-stacked {
  padding-left: 0px;
}
.form-stacked form {
  margin-bottom: 0px;
}
.center {
  text-align: center;
}
/*.actions {padding-left:0;}*/
/* BEGIN */
html, body, #main-container {
  height: 100%;
}
body {
  color: #565656;
  font: 12px/20px "Droid Sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 12px;
}
/* Typography */
a {
  color: inherit;
  text-decoration: inherit;
}
a:hover, a:focus {
  color: #466779;
}
h1 {
  font-size: 25px;
}
h2 {
  font-size: 24px;
}
.header h2 {
  line-height: 24px;
}
h3 {
  font-size: 20px;
}
h4 {
  font-size: 16px;
}
h5 {
  font-size: 14px;
}
h6 {
  font-size: 12px;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  margin-bottom: .5em;
}
/*h1,h2,h3{font-weight:400;}*/
h4,
h5,
h6,
b,
strong,
caption,
th,
thead,
dt,
legend {
  font-weight: 700;
}
ol, ul {
  list-style: disc;
  padding-left: 15px;
  margin-bottom: 10px;
}
ol {
  list-style: decimal;
}
ol li, ul li {
  margin-bottom: 3px;
}
p {
  margin-bottom: 10px;
}
.strike {
  text-decoration: line-through;
}
body > #main-container {
  height: auto;
  min-height: 100%;
}
#main-container {
  padding-bottom: 180px;
  text-align: left;
  background: url(<%= asset_path 'global/noise.png' %>) repeat top left;
}

.pad-8 {
  padding: 8px;
}
.pad-16 {
  padding: 16px;
}
header {
  background: transparent url('/images/global/bg-tile.jpg') repeat-x top left;
  color: #FAFAFA;
}
#header-top-row {
  height: 56px;
}
header .inner {
  margin: 0px auto;
  height: 200px;
}
#nameplate {
  background: url('/images/global/nameplate.png') no-repeat scroll top left;
  text-indent: -3000px;
  margin: 6px 0 0 0;
}
#nameplate a:hover, #nameplate a {
  display: block;
  width: 166px;
  height: 43px;
  background-color: transparent;
}
ul#top-navbar {
  margin: 16px -12px 8px 8px;
  padding: 0;
  width: auto;
  list-style: none;
  text-align: right;
}
ul#top-navbar li {
  display: inline;
  position: relative;
  list-style: none outside none;
  margin: 0;
}
ul#top-navbar li a {
  color: #F0F0F0;
  display: inline-block;
  padding: 8px 12px;
  text-decoration: none;
}
ul#top-navbar li a:hover {
  background-color: #2E5069;
  border-radius: 6px;
}
/*  global navbar */
ul#global-navbar {
  margin-top: 0px;
  padding: 0;
  width: auto;
  list-style: none;
}
ul#global-navbar li {
  display: inline;
}
ul#global-navbar li a {
  margin: 0;
  font-weight: bold;
  color: #F0F0F0;
  display: inline-block;
  padding: 10px;
  line-height: 20px;
  text-decoration: none;
}
ul#global-navbar li a.selected, ul#global-navbar li a:hover {
  background-color: #1C2A35;
}
#main {
  padding: 16px 0;
}
.out {
  outline: solid 1px lime;
  margin-bottom: 16px;
}
footer#main-footer {
  border-top: solid 	4px #999;
  height: 176px;
  position: relative;
  margin: -180px 0 0 0;
  padding: 0;
  clear: both;
  background-image: -moz-linear-gradient(top, #cccccc, transparent);
  background-image: -webkit-gradient(linear, center top, center bottom, from(#cccccc), to(transparent));
  background-image: -o-linear-gradient(top, #cccccc, transparent);
  background-image: linear-gradient(top, #cccccc, transparent);
}
footer#main-footer .inner {
  margin: 0px auto;
  padding: 0;
}
